@prefix : <http://www.modsem.org/ontologies/MusicGenreOntology#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@base <http://www.modsem.org/ontologies/MusicGenreOntology#> .

<http://www.modsem.org/ontologies/MusicGenreOntology> rdf:type owl:Ontology ;
                                                       dc:creator "Lorenzo Grassi" ,
                                                                  "Samuele Monasterolo" ;
                                                       dc:date "04/01/2024" ;
                                                       dc:description "Ontologia focalizzata a descrivere principlamente i generi e sottogeneri musicali e rispettivi artisti"@it ;
                                                       dc:title "Music Genre Ontology"@en ;
                                                       vann:preferredNamespacePrefix "mgo" ;
                                                       vann:preferredNamespaceUri "http://www.modsem.org/ontologies/MusicGenreOntology#" ;
                                                       rdfs:label "MusicGenreOntology" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.modsem.org/ontologies/MusicGenreOntology#campiona
:campiona rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdf:type owl:TransitiveProperty ;
          rdfs:domain :Brano ;
          rdfs:range :Brano ;
          rdfs:comment "un brano campiona (un segmento) di un altro brano"@it ;
          rdfs:label "campiona"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#caratterizza
:caratterizza rdf:type owl:ObjectProperty ;
              owl:inverseOf :caratterizzatoDa ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :StrumentoMusicale ;
              rdfs:range :Genere ;
              rdfs:comment "uno strumento musicale caratterizza un genere musicale"@it ;
              rdfs:label "caratterizza" .


###  http://www.modsem.org/ontologies/MusicGenreOntology#caratterizzatoDa
:caratterizzatoDa rdf:type owl:ObjectProperty ,
                           owl:InverseFunctionalProperty ;
                  rdfs:domain :Genere ;
                  rdfs:range :StrumentoMusicale ;
                  rdfs:comment "un genere è caratterizzato da uno strumento musicale"@it ;
                  rdfs:label "caratterizzato da"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#collaboraCon
:collaboraCon rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdf:type owl:IrreflexiveProperty ;
              rdfs:domain :Artista ;
              rdfs:range :Artista ;
              rdfs:comment "un artista collabora con un altro artista"@it ;
              rdfs:label "collabora con"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#collaboraSu
:collaboraSu rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Artista ;
             rdfs:range :Brano .


###  http://www.modsem.org/ontologies/MusicGenreOntology#contieneDiscografia
:contieneDiscografia rdf:type owl:ObjectProperty ;
                     owl:inverseOf :haGenere ;
                     rdfs:domain :Genere ;
                     rdfs:range :Brano ;
                     rdfs:comment "un genere contiene una lista di brani"@it ;
                     rdfs:label "contiene discografia"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#contieneRuolo
:contieneRuolo rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :ruoloIn ;
               rdfs:domain :Brano ;
               rdfs:range :Ruolo ;
               rdfs:comment "un brano contiene un ruolo interpretato da un artista"@it ;
               rdfs:label "contiene ruolo"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#haGenere
:haGenere rdf:type owl:ObjectProperty ;
          rdfs:domain :Brano ;
          rdfs:range :Genere ;
          owl:propertyChainAxiom ( :utilizza
                                   :caratterizza
                                 ) ;
          rdfs:comment "un brano ha un genere"@it ;
          rdfs:label "ha genere"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#haRuolo
:haRuolo rdf:type owl:ObjectProperty ;
         owl:inverseOf :interpretatoDa ;
         rdfs:domain :Artista ;
         rdfs:range :Ruolo ;
         rdfs:comment "un artista ha un ruolo all'interno di un brano"@it ;
         rdfs:label "ha ruolo"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#interpretatoDa
:interpretatoDa rdf:type owl:ObjectProperty ;
                rdfs:domain :Ruolo ;
                rdfs:range :Artista ;
                rdfs:comment "il ruolo è interpretato da un artista"@it ;
                rdfs:label "interpretato da"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ispira
:ispira rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        owl:inverseOf :ispiratoDa ;
        rdf:type owl:TransitiveProperty ;
        rdfs:domain :Artista ;
        rdfs:range :Artista ;
        rdfs:label "ispira"@it ,
                   "un artista ispira un altro artista"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ispiratoDa
:ispiratoDa rdf:type owl:ObjectProperty ,
                     owl:TransitiveProperty ;
            rdfs:domain :Artista ;
            rdfs:range :Artista ;
            rdfs:comment "un artista è ispirato da un altro artista"@it ;
            rdfs:label "ispirato da"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#rilascia
:rilascia rdf:type owl:ObjectProperty ;
          owl:inverseOf :rilasciatoDa ;
          rdfs:domain :Artista ;
          rdfs:range :Brano ;
          rdfs:comment "un artista rilascia un brano"@it ;
          rdfs:label "rilascia"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#rilasciatoDa
:rilasciatoDa rdf:type owl:ObjectProperty ;
              rdfs:domain :Brano ;
              rdfs:range :Artista ;
              rdfs:comment "un brano è rilasciato da un artista"@it ;
              rdfs:label "rilasciato da"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ruoloIn
:ruoloIn rdf:type owl:ObjectProperty ;
         rdfs:domain :Ruolo ;
         rdfs:range :Brano ;
         rdfs:comment "un ruolo (interpretato da un artista) è espresso in un brano"@it ;
         rdfs:label "ruolo in"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#suona
:suona rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf owl:topObjectProperty ;
       rdfs:domain :Artista ;
       rdfs:range :StrumentoMusicale ;
       owl:propertyChainAxiom ( :rilascia
                                :utilizza
                              ) ;
       rdfs:comment "un artista ha rilasciato una canzone in cui viene suonato uno strumento musicale"@it ;
       rdfs:label "suona"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#utilizza
:utilizza rdf:type owl:ObjectProperty ;
          rdfs:domain :Brano ;
          rdfs:range :StrumentoMusicale ;
          rdfs:comment "un brano utilizza uno strumento musicale" ;
          rdfs:label "utilizza"@it .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/ontology/mo/bpm
<http://purl.org/ontology/mo/bpm> rdf:type owl:DatatypeProperty ;
                                  owl:equivalentProperty :BPM .


###  http://purl.org/ontology/mo/key
<http://purl.org/ontology/mo/key> rdf:type owl:DatatypeProperty ;
                                  owl:equivalentProperty :chiave .


###  http://www.modsem.org/ontologies/MusicGenreOntology#BPM
:BPM rdf:type owl:DatatypeProperty ;
     rdfs:domain :Brano ;
     rdfs:range xsd:int ;
     rdfs:comment "I battiti per minuto (bpm) sono l'unità di misura di frequenza, usata principalmente per l'indicazione di quanti beat ci sono in un minuto in musica e per la misura della frequenza cardiaca."@it ;
     rdfs:label "BPM"@en .


###  http://www.modsem.org/ontologies/MusicGenreOntology#chiave
:chiave rdf:type owl:DatatypeProperty ;
        rdfs:domain :Brano ;
        rdfs:range xsd:string ;
        rdfs:comment "La chiave, nella notazione musicale, è un simbolo posto sul pentagramma che ha la funzione di fissare la posizione delle note."@it ;
        rdfs:label "chiave"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#durata
:durata rdf:type owl:DatatypeProperty ;
        rdfs:domain :Brano ;
        rdfs:range xsd:int ;
        dc:description "Indica la durata del brano espressa in secondi" ;
        rdfs:label "durata" .


###  http://www.modsem.org/ontologies/MusicGenreOntology#nazionalità
:nazionalità rdf:type owl:DatatypeProperty ;
             rdfs:domain :Artista ;
             rdfs:range xsd:string ;
             rdfs:comment "nazionalità dell'artista"@it ;
             rdfs:label "nazionalità"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#nomeCompleto
:nomeCompleto rdf:type owl:DatatypeProperty ;
              rdfs:domain :Artista ;
              rdfs:range xsd:string ;
              rdfs:comment "indica il nome completo dell'artista, nel caso in cui sia conosciuto con un nome d'arte"@it ;
              rdfs:label "nome completo"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#topRanking
:topRanking rdf:type owl:DatatypeProperty ;
            rdfs:domain :Brano ;
            rdfs:range xsd:int ;
            dc:description "posizione massima del brano raggiunta nella classifica USA" ;
            rdfs:label "top ranking" .


#################################################################
#    Classes
#################################################################

###  http://purl.org/ontology/mo/Genre
<http://purl.org/ontology/mo/Genre> rdf:type owl:Class ;
                                    owl:equivalentClass :Genere .


###  http://purl.org/ontology/mo/Instrument
<http://purl.org/ontology/mo/Instrument> rdf:type owl:Class ;
                                         owl:equivalentClass :StrumentoMusicale .


###  http://purl.org/ontology/mo/MusicArtist
<http://purl.org/ontology/mo/MusicArtist> rdf:type owl:Class ;
                                          owl:equivalentClass :Artista .


###  http://purl.org/ontology/mo/Record
<http://purl.org/ontology/mo/Record> rdf:type owl:Class ;
                                     owl:equivalentClass :Brano .


###  http://www.modsem.org/ontologies/MusicGenreOntology#Artista
:Artista rdf:type owl:Class ;
         rdfs:comment "persona che svolge una determinata attività in musica, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
         rdfs:label "Artista"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaClassico
:ArtistaClassico rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :rilascia ;
                                       owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty :haGenere ;
                                                                                   owl:someValuesFrom :Classica
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ]
                                     ] ;
                 rdfs:subClassOf :Artista ;
                 rdfs:comment "persona che svolge una determinata attività in musica classica, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
                 rdfs:label "Artista classico"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaCountry
:ArtistaCountry rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :rilascia ;
                                      owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty :haGenere ;
                                                                                  owl:someValuesFrom :Country
                                                                                ]
                                                                              ) ;
                                                           rdf:type owl:Class
                                                         ]
                                    ] ;
                rdfs:subClassOf :Artista ;
                rdfs:comment "persona che svolge una determinata attività in musica country, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
                rdfs:label "Artista country" .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaDisco
:ArtistaDisco rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :rilascia ;
                                    owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty :haGenere ;
                                                                                owl:someValuesFrom :Disco
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ;
              rdfs:subClassOf :ArtistaEDM ;
              rdfs:comment "persona che svolge una determinata attività in musica disco, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
              rdfs:label "Artista disco" .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaEDM
:ArtistaEDM rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :rilascia ;
                                  owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :haGenere ;
                                                                              owl:someValuesFrom :EDM
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ]
                                ] ;
            rdfs:subClassOf :Artista ;
            rdfs:comment "persona che svolge una determinata attività in musica EDM, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
            rdfs:label "Artista EDM"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaHouse
:ArtistaHouse rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :rilascia ;
                                    owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty :haGenere ;
                                                                                owl:someValuesFrom :House
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ;
              rdfs:subClassOf :ArtistaEDM ;
              rdfs:comment "persona che svolge una determinata attività in musica house, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
              rdfs:label "Artista house"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaKpop
:ArtistaKpop rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :rilascia ;
                                   owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :haGenere ;
                                                                               owl:someValuesFrom :KPop
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ]
                                 ] ;
             rdfs:subClassOf :ArtistaPop ;
             rdfs:comment "persona che svolge una determinata attività in musica Kpop, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
             rdfs:label "Artista KPop"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaMelodicTechno
:ArtistaMelodicTechno rdf:type owl:Class ;
                      owl:equivalentClass [ rdf:type owl:Restriction ;
                                            owl:onProperty :rilascia ;
                                            owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                                      [ rdf:type owl:Restriction ;
                                                                                        owl:onProperty :haGenere ;
                                                                                        owl:someValuesFrom :MelodicTechno
                                                                                      ]
                                                                                    ) ;
                                                                 rdf:type owl:Class
                                                               ]
                                          ] ;
                      rdfs:subClassOf :ArtistaTechno ;
                      rdfs:comment "persona che svolge una determinata attività in musica melodic techno, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
                      rdfs:label "Artista melodic techno"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaMinimalHouse
:ArtistaMinimalHouse rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :rilascia ;
                                           owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                                     [ rdf:type owl:Restriction ;
                                                                                       owl:onProperty :haGenere ;
                                                                                       owl:someValuesFrom :MinimalHouse
                                                                                     ]
                                                                                   ) ;
                                                                rdf:type owl:Class
                                                              ]
                                         ] ;
                     rdfs:subClassOf :ArtistaHouse ;
                     rdfs:comment "persona che svolge una determinata attività in musica minimal house, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
                     rdfs:label "Artista minimal house"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaPop
:ArtistaPop rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :rilascia ;
                                  owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :haGenere ;
                                                                              owl:someValuesFrom :Pop
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ]
                                ] ;
            rdfs:subClassOf :Artista ;
            rdfs:comment "persona che svolge una determinata attività in musica pop, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
            rdfs:label "Artista pop"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaRap
:ArtistaRap rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :rilascia ;
                                  owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :haGenere ;
                                                                              owl:someValuesFrom :Rap
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ]
                                ] ;
            rdfs:subClassOf :Artista ;
            rdfs:comment "persona che svolge una determinata attività in musica rap, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
            rdfs:label "Artista rap"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#ArtistaTechno
:ArtistaTechno rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :rilascia ;
                                     owl:someValuesFrom [ owl:intersectionOf ( :Brano
                                                                               [ rdf:type owl:Restriction ;
                                                                                 owl:onProperty :haGenere ;
                                                                                 owl:someValuesFrom :Techno
                                                                               ]
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ]
                                   ] ;
               rdfs:subClassOf :ArtistaEDM ;
               rdfs:comment "persona che svolge una determinata attività in musica techno, inerente alla creazione o all'esecuzione di composizioni strumentali, vocali, corali."@it ;
               rdfs:label "Artista techno"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#Brano
:Brano rdf:type owl:Class ;
       rdfs:comment "Un brano è una composizione vocale scritta per una o più voci perlopiù con accompagnamento strumentale."@it ;
       rdfs:label "Brano"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#BranoFamoso
:BranoFamoso rdf:type owl:Class ;
             rdfs:subClassOf :Brano ;
             rdfs:comment "brano il cui top ranking è maggiore o uguale a 5"@it ;
             rdfs:label "Brano famoso" .


###  http://www.modsem.org/ontologies/MusicGenreOntology#BranoPolitrumentale
:BranoPolitrumentale rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :utilizza ;
                                           owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                           owl:onClass :StrumentoMusicale
                                         ] ;
                     rdfs:subClassOf :Brano ;
                     rdfs:comment "Brano che utilizza almeno 3 strumenti musicali diveri"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#Classica
:Classica rdf:type owl:Class ;
          rdfs:subClassOf :Genere ;
          rdfs:comment "Genere relativo alla musica classica"@it ;
          rdfs:label "Classica"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf :Genere ;
         rdfs:comment "Genere relativo alla musica country"@it ;
         rdfs:label "Country"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#Disco
:Disco rdf:type owl:Class ;
       rdfs:subClassOf :EDM ;
       rdfs:comment "Genere relativo alla musica disco"@it ;
       rdfs:label "Disco"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#EDM
:EDM rdf:type owl:Class ;
     rdfs:subClassOf :Genere ;
     rdfs:comment "Genere relativo alla musica EDM"@it ;
     rdfs:label "EDM"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#Genere
:Genere rdf:type owl:Class ;
        rdfs:comment "Genere musicale generale"@it ;
        rdfs:label "Genere"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#House
:House rdf:type owl:Class ;
       rdfs:subClassOf :EDM ;
       rdfs:comment "Genere relativo alla musica house"@it ;
       rdfs:label "House"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#KPop
:KPop rdf:type owl:Class ;
      rdfs:subClassOf :Pop ;
      rdfs:comment "Genere relativo alla musica kpop"@it ;
      rdfs:label "KPop"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#MelodicTechno
:MelodicTechno rdf:type owl:Class ;
               rdfs:subClassOf :Techno ;
               rdfs:comment "Genere relativo alla musica melodic techno"@it ;
               rdfs:label "Melodic Techno"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#MinimalHouse
:MinimalHouse rdf:type owl:Class ;
              rdfs:subClassOf :House ;
              rdfs:comment "Genere relativo alla musica minimal house"@it ;
              rdfs:label "Minimal House"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#Pop
:Pop rdf:type owl:Class ;
     rdfs:subClassOf :Genere ;
     rdfs:comment "Genere relativo alla musica pop"@it ;
     rdfs:label "Pop"@it .


###  http://www.modsem.org/ontologies/MusicGenreOntology#Rap
:Rap rdf:type owl:Class ;
     rdfs:subClassOf :Genere ;
